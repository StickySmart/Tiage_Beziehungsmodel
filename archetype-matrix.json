  "archetypes": {
    "#A1": {
      "id": "#A1",
      "key": "single",
      "name": "Single",
      "color": "#E63946",
      "colorLight": "#FFB4A2",
      "colorDark": "#9D2933",
      "shortDescription": "Fokus auf Selbstentwicklung, temporär oder bewusst ungebunden. Maximale persönliche Freiheit.",
      "fullDescription": "Der Single-Archetyp lebt primär für sich selbst und eigene Projekte. Dies kann eine bewusste Lebensphase oder eine dauerhafte Entscheidung sein."
    },
    "#A2": {
      "id": "#A2",
      "key": "duo",
      "name": "Duo",
      "color": "#E84393",
      "colorLight": "#FDA7DF",
      "colorDark": "#B53471",
      "shortDescription": "Klassische monogame Zweierbeziehung. Exklusivität, Tiefe und Verbindlichkeit mit einer Person.",
      "fullDescription": "Das Duo-Modell repräsentiert die traditionelle monogame Partnerschaft: Eine tiefe, exklusive Verbindung zwischen zwei Menschen."
    },
    "#A3": {
      "id": "#A3",
      "key": "duo_flex",
      "name": "Duo-Flex",
      "color": "#FF6B6B",
      "colorLight": "#FFA5A5",
      "colorDark": "#CC5555",
      "shortDescription": "Primär Zweierbeziehung mit situativer Öffnung. Monogamisch mit Flexibilität nach Absprache.",
      "fullDescription": "Das Duo-Flex-Modell vereint die Stabilität einer Hauptbeziehung mit situativer Offenheit."
    },
    "#A4": {
      "id": "#A4",
      "key": "solopoly",
      "name": "Solopoly",
      "color": "#2A9D8F",
      "colorLight": "#A8DADC",
      "colorDark": "#1D7069",
      "shortDescription": "Polyamor mit maximaler Autonomie. Keine Primärbeziehung, kein Zusammenleben.",
      "fullDescription": "Solopoly vereint polyamore Offenheit mit konsequenter Autonomie. Der Fokus liegt auf dem 'Solo'."
    },
    "#A5": {
      "id": "#A5",
      "key": "polyamor",
      "name": "Polyamor",
      "color": "#F4A261",
      "colorLight": "#FFE5B4",
      "colorDark": "#C07B3E",
      "shortDescription": "Strukturierte, ethische Mehrfachbeziehungen mit Fokus auf tiefe romantische Bindungen.",
      "fullDescription": "Klassisches Polyamory mit Betonung auf Ethik, Kommunikation und oft einer hierarchischen Struktur."
    },
    "#A6": {
      "id": "#A6",
      "key": "ra",
      "name": "RA",
      "color": "#9B5DE5",
      "colorLight": "#D4A5FF",
      "colorDark": "#7340B0",
      "shortDescription": "RA - Ablehnung aller Beziehungs-Hierarchien und Labels.",
      "fullDescription": "RA lehnt gesellschaftliche Beziehungsnormen ab. Jede Verbindung wird individuell definiert ohne vorgegebene Kategorien."
    },
    "#A7": {
      "id": "#A7",
      "key": "lat",
      "name": "LAT",
      "color": "#06D6A0",
      "colorLight": "#B5EAD7",
      "colorDark": "#048A65",
      "shortDescription": "Living Apart Together - Feste Partnerschaft ohne Zusammenleben.",
      "fullDescription": "LAT beschreibt eine verbindliche Beziehung, bei der beide Partner bewusst getrennte Wohnungen behalten."
    },
    "#A8": {
      "id": "#A8",
      "key": "aromantisch",
      "name": "Aromantisch",
      "color": "#118AB2",
      "colorLight": "#83C5D8",
      "colorDark": "#0C5F7A",
      "shortDescription": "Fokus auf platonische Verbindungen ohne romantische Komponente.",
      "fullDescription": "Aromantische Menschen erleben keine oder wenig romantische Anziehung, können aber tiefe platonische Bindungen haben."
    }
  },

